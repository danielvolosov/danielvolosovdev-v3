{
  "name": "danielvolosovdev-v3",
  "description": "Daniel Volosov's personal website",
  "version": "0.1.0",
  "private": true,
  "homepage": "http://danielvolosov.dev",
  "dependencies": {
    "@rebass/preset": "^4.0.5",
    "emotion-theming": "^10.0.27",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-markdown": "^4.3.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.1",
    "rebass": "^4.0.7"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "format": "prettier --write \"**/*.+(js|jsx|json|css|md)\""
  },
  "prettier": {
    "printWidth": 85,
    "arrowParens": "always",
    "semi": true,
    "tabWidth": 2,
    "bracketSpacing": false,
    "trailingComma": "es5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "prettier --write \"**/*.+(js|jsx|json|css|md)\""
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "css-loader": "^3.5.3",
    "eslint": "^6.6.0",
    "husky": "^4.2.5",
    "prettier": "^2.0.5"
  }
}
